{
  "name": "expressservertemplate",
  "version": "1.0.1",
  "description": "Template de serveur Express",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env nodemon src/server.js --exec babel-node",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "build": "babel src -d dist --ignore ./src/public --copy-files",
    "test": "jest",
    "commit": "git-cz",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/O-clock-Dev/ExpressServerTemplate.git"
  },
  "keywords": [
    "template",
    "nodejs",
    "express",
    "js"
  ],
  "author": "SÃ©bastien Lampazona",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/O-clock-Dev/ExpressServerTemplate/issues"
  },
  "homepage": "https://github.com/O-clock-Dev/ExpressServerTemplate#readme",
  "dependencies": {
    "@sentry/node": "^7.12.1",
    "@sentry/tracing": "^7.12.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "http-status": "^1.5.3",
    "keycloak-connect": "^19.0.1",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.5.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/node": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.19.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "babel-jest": "^29.0.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.6",
    "commitizen": "^4.2.5",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "release-please": "^14.5.0",
    "supertest": "^6.2.4"
  }
}
